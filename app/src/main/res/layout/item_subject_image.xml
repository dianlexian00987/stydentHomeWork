<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/subjective_answer_frame_three"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/x20"
    android:paddingRight="@dimen/x20">

    <ImageView
        android:id="@+id/subjective_img_three"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/x300"
        android:scaleType="centerCrop"
        android:src="@mipmap/ic_launcher" />

    <RelativeLayout
        android:id="@+id/subjective_del_layout_three"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:background="#99000000">

        <ImageView
            android:id="@+id/subjective_del_three"
            android:layout_width="@dimen/x40"
            android:layout_height="@dimen/x40"
            android:layout_centerInParent="true"
            android:layout_marginTop="@dimen/x9"
            android:layout_marginBottom="@dimen/x9"
            android:src="@mipmap/del_litter" />
    </RelativeLayout>
</FrameLayout>

